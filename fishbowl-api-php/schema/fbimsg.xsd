    </xsd:complexType>

    <xsd:complexType name="HoldPickItemRsType">
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <xsd:complexType name="FinishPickRqType">
        <xsd:sequence>
            <xsd:element ref="Pick"                 minOccurs="1" maxOccurs="1" />
            <xsd:element name="Slot"                type="STRTYPE" minOccurs="1" maxOccurs="1" />
            <xsd:element name="LocNum"              type="STRTYPE" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="FinishPickRsType">
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <!-- ============================================================================= -->
    <!-- Pick                                                                          -->
    <!-- ============================================================================= -->


    <xsd:complexType name="YieldRqType">
        <xsd:sequence>
            <xsd:element name="TagNum"              type="STRTYPE"  minOccurs="1" maxOccurs="1" />
            <xsd:element name="DestinationTagNum"   type="STRTYPE"  minOccurs="1" maxOccurs="1" />
            <xsd:element name="Location"            type="STRTYPE"  minOccurs="0" maxOccurs="1" />
            <xsd:element name="Quantity"            type="STRTYPE"  minOccurs="1" maxOccurs="1"/>
            <xsd:element name="Ref"                 type="STRTYPE" />
            <xsd:element ref="SerialNumbers"        minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="DestinationTagType" >
        <xsd:sequence>
            <xsd:element ref="Tag"     minOccurs="1" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="YieldRsType">
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <!-- ============================================================================= -->
    <!-- Order Queries -->
    <!-- ============================================================================= -->
    <xsd:complexType name="GetSOListRqType">
        <xsd:sequence>
            <xsd:element name="LocationGroup"   type="STRTYPE" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="GetSOListRsType">
        <xsd:sequence>
            <xsd:element ref="SalesOrder" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <xsd:complexType name="GetPOListRqType">
        <xsd:sequence>
            <xsd:element name="LocationGroup"   type="STRTYPE" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="GetPOListRsType">
        <xsd:sequence>
            <xsd:element ref="PurchaseOrder" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

