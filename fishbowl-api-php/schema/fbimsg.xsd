        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="SaveSORsType">
        <xsd:sequence>
            <xsd:element ref="SalesOrder" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <xsd:complexType name="LoadSORqType">
        <xsd:sequence>
            <xsd:element name="Number"      type="STRTYPE"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="LoadSORsType">
        <xsd:sequence>
            <xsd:element ref="SalesOrder"/>
        </xsd:sequence>
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <xsd:complexType name="SavePORqType">
        <xsd:sequence>
            <xsd:element ref="PurchaseOrder"/>
            <xsd:element name="IssueFlag"   type="BOOLTYPE" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="SavePORsType">
        <xsd:sequence>
            <xsd:element ref="PurchaseOrder"/>
        </xsd:sequence>
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <xsd:complexType name="LoadPORqType">
        <xsd:sequence>
            <xsd:element name="Number"      type="STRTYPE"/>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="LoadPORsType">
        <xsd:sequence>
            <xsd:element ref="PurchaseOrder"/>
        </xsd:sequence>
        <xsd:attributeGroup ref="CommonRsAttrs"/>
