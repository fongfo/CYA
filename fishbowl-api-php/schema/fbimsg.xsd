    <!-- ============================================================================= -->
    <!-- Part Queries -->
    <!-- ============================================================================= -->


    <xsd:complexType name="PartQueryRqType">
        <xsd:sequence>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:element ref="PartNum"/>
                <xsd:element ref="UPC"/>
                <xsd:element name="VendorPartNumber" type="STRTYPE" />
            </xsd:choice>
            <xsd:element name="LocationGroup" type="STRTYPE" minOccurs="1" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PartQueryRsType">
        <xsd:sequence>
            <xsd:element ref="Part" minOccurs="0"/>
            <xsd:element ref="Tag"  minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <!-- ============================================================================= -->
    <!-- Part Cost Queries -->
    <!-- ============================================================================= -->

    <xsd:complexType name="PartCostRqType">
        <xsd:sequence>
            <xsd:choice minOccurs="1" maxOccurs="1">
                <xsd:element ref="PartNum"/>
                <xsd:element ref="UPC"/>
                <xsd:element name="VendorPartNumber" type="STRTYPE" />
            </xsd:choice>
            <xsd:element name="LocationGroup" type="STRTYPE" minOccurs="0" maxOccurs="1" />
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PartCostRsType">
        <xsd:sequence>
            <xsd:element ref="PartNum"                          minOccurs="1" />
