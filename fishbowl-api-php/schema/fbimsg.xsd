                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ContainingPartNum"      type="STRTYPE"  minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Transfer Order contains this part number.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ContainingPartDescription"      type="STRTYPE"  minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Transfer Order contains this part description.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element ref="CustomField"      minOccurs="0" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Custom field to search by.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SortBy"      type="STRTYPE"  minOccurs="0" maxOccurs="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">How to sort the Transfer Orders.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>


    <xsd:complexType name="GetTransferOrderListRsType">
        <xsd:sequence>
            <xsd:group ref="OrderNumber" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
        <xsd:attributeGroup ref="CommonRsAttrs"/>
    </xsd:complexType>

    <xsd:group name="OrderNumber">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">List of order numbers</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Number" type="STRTYPE" minOccurs="1" maxOccurs="unbounded">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">The order number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:group>

</xsd:schema>
